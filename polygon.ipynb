   "source": [
    "import turtle\n",
    "turtle.Screen().bgcolor(\"black\")\n",
    "turtle.Screen().setup(500,500)\n",
    "\n",
    "pen=turtle.Turtle()\n",
    "pen.pencolor(\"white\")\n",
    "numberside=6\n",
    "length=100\n",
    "angle=360/numberside\n",
    "for i in range(numberside):\n",
    "    pen . forward(length)\n",
    "    pen . right(angle)\n",
    "turtle.done()"
   ]
